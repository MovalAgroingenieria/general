<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <!-- Add ext_collector payment method -->
        <record id="payment_method_ext_collector" model="account.payment.method">
            <field name="name">External Colletor</field>
            <field name="code">ext_collector</field>
            <field name="payment_type">inbound</field>
            <field name="prefix">extcoll_</field>
            <field name="bank_account_required">False</field>
            <field name="mandate_required">False</field>
        </record>

        <!-- Sequence for external collector ref -->
        <record model="ir.sequence" id="external_collector_ref_seq">
            <field name="name">External Collector Reference</field>
            <field name="code">external_collector_ref_seq</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>