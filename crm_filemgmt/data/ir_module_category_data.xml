<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="crm_filemgmt.module_category" model="ir.module.category">
        <field name="name">File Management</field>
    </record>

    <record id="resfilecategory_internal_file"
            model="res.file.category" forcecreate="False">
        <field name="alphanum_code">Internal Files</field>
        <field name="is_readonly">True</field>
    </record>

    <record id="resfilecategory_external_file"
            model="res.file.category" forcecreate="False">
        <field name="alphanum_code">External Files</field>
        <field name="is_readonly">True</field>
    </record>

</odoo>