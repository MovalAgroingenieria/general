        <record id="res_file_stage_draft" model="res.file.stage">
            <field name="name">Draft</field>
            <field name="sequence">10</field>
            <field name="fold">False</field>
            <field name="is_closing_stage">False</field>
        </record>

        <record id="res_file_stage_inprogress" model="res.file.stage">
