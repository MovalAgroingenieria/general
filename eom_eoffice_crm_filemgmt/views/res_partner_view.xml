<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_res_partner_filter" model="ir.ui.view">
        <field name="name">Partners (search view)</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_res_partner_filter"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='group_by']" position="before">
                <filter string="Created by Auth DNIe: Yes" name="created_by_authdnie_yes"
                    domain="[('created_by_authdnie','=',True)]"/>
                <filter string="Created by Auth DNIe: No" name="created_by_authdnie_no"
                    domain="[('created_by_authdnie','=',False)]"/>
                <separator/>
            </xpath>
        </field>
    </record>

</odoo>