<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <template id="assets_backend"
              name="backend_resources_eom_eoffice"
              inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet"
             href="/eom_eoffice/static/src/css/eom_eoffice.css"/>
        </xpath>
    </template>

    <template id="assets_frontend"
              name="frontend_resources_eom_eoffice"
              inherit_id="website.assets_frontend">
    <xpath expr="." position="inside">
        <link rel="stylesheet"
             href="/eom_eoffice/static/src/css/eom_eoffice_website.css"/>
    </xpath>
</template>

</odoo>