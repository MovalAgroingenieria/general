<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="law_water_quality.module_category_laboratory_analysis_waters"
                model="ir.module.category">
            <field name="name">Laboratory Analysis Waters</field>
        </record>

        <record id="law_water_quality.group_law_user" model="res.groups">
            <field name="name">Law User</field>
            <field name="category_id"
                ref="law_water_quality.module_category_laboratory_analysis_waters"/>
            <field name="implied_ids"
                eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record id="law_water_quality.group_law_manager" model="res.groups">
            <field name="name">Law Manager</field>
            <field name="category_id"
                ref="law_water_quality.module_category_laboratory_analysis_waters"/>
            <field name="implied_ids"
                eval="[(4, ref('law_water_quality.group_law_user'))]"/>
            <field name="users"
                eval="[(4, ref('base.user_root'))]"/>
        </record>

    </data>
</odoo>