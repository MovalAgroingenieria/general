<odoo>
    <record id="view_hr_feedback_note_type_tree" model="ir.ui.view">
        <field name="name">hr.feedback.note.type.tree</field>
        <field name="model">hr.employee.feedback.note.type</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_hr_feedback_note_type_form" model="ir.ui.view">
        <field name="name">hr.feedback.note.type.form</field>
        <field name="model">hr.employee.feedback.note.type</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_hr_feedback_note_type" model="ir.actions.act_window">
        <field name="name">Feedback Note Types</field>
        <field name="res_model">hr.employee.feedback.note.type</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="menu_hr_feedback_note_type"
    name="Feedback Note Types"
    parent="menu_hr_feedback_root"
    action="hr_employee_feedback_notes.action_hr_feedback_note_type"
    sequence="92"
    groups="hr_employee_feedback_notes.group_hr_feedback_manager"/>

</odoo>
