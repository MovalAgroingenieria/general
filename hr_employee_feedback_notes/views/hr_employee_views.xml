<odoo>
  <record id="view_hr_employee_form_feedback_note" model="ir.ui.view">
      <field name="name">hr.employee.form.inherit.feedback.note</field>
      <field name="model">hr.employee</field>
      <field name="inherit_id" ref="hr.view_employee_form"/>
      <field name="arch" type="xml">
          <notebook position="inside">
              <page string="Feedback Notes" groups="hr_employee_feedback_notes.group_hr_feedback_manager">
                  <field name="feedback_note_ids">
                      <tree>
                          <field name="date"/>
                          <field name="name"/>
                          <field name="note_type_id"/>
                      </tree>
                  </field>
              </page>
          </notebook>
      </field>
  </record>
</odoo>