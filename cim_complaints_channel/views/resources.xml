<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <template id="assets_backend"
              name="backend_resources_cim_complaints_channel"
              inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet"
             href="/cim_complaints_channel/static/src/css/cim_complaints_channel.css"/>
            <link rel="stylesheet"
             href="/cim_complaints_channel/static/src/less/cim_complaints_channel.less"/>             
        </xpath>
    </template>

    <template id="assets_frontend"
              name="frontend_resources_cim_complaints_channel"
              inherit_id="website_form.assets_frontend">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/cim_complaints_channel/static/src/js/button_send.js"/>
        </xpath>
    </template>
    
</odoo>