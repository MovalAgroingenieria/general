<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="cookie_service_script" inherit_id="website.layout">
        <xpath expr="//header" position="inside">
            <t t-set="external_script"
                t-value="self.env['ir.config_parameter'].sudo().get_param(
            'cookie_notice_external.cookie_service_script')"/>
            <t t-raw="external_script"/>
        </xpath>
    </template>

</odoo>
