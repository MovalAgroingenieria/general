<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="cookie_service_script" inherit_id="website.layout">
        <xpath expr="//header" position="inside">
            <t t-set="external_script"
                t-value="request.env['ir.values'].get_default('website.config.settings', 'cookie_service_script')"/>
            <t t-raw="external_script"/>
        </xpath>
    </template>

</odoo>
