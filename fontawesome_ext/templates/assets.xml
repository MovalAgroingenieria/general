<?xml version="1.0" encoding="utf-8" ?>

<odoo>

    <template id="assets_ext" inherit_id="web.assets_common" name="static_resources_fontawesome_ext">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/css"
                href="/fontawesome_ext/static/lib/custom_fontawsome/css/moval_iconset.css"/>
        </xpath>
    </template>

    <template id="minimal_layout_fontawesome_ext" inherit_id="web.minimal_layout" name="Add fontawesome_ext to reports">
        <xpath expr="//head" position="inside">
            <link rel="stylesheet" type="text/css"
                href="/fontawesome_ext/static/lib/custom_fontawsome/css/moval_iconset.css"/>
        </xpath>
    </template>

</odoo>
