<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <!-- Add DIPUCR payment method -->
        <record id="payment_method_dipucr" model="account.payment.method">
            <field name="name">Tax management. Ciudad Real County Council (DipuCR)</field>
            <field name="code">debit_dipucr</field>
            <field name="payment_type">inbound</field>
            <field name="prefix">dipucr_</field>
            <field name="bank_account_required">False</field>
            <field name="mandate_required">False</field>
            <!-- Force id (needed to add payment mode)
            <field name="id">6</field> -->
        </record>

        <!-- Sequence for value list_ref -->
        <record model="ir.sequence" id="dipucr_value_list_refnum_seq">
            <field name="name">Value list ref for DipuCR</field>
            <field name="code">dipucr_seq_value_list_refnum</field>
            <field name="prefix"></field>
            <field name="padding">6</field>
        </record>

    </data>
</odoo>